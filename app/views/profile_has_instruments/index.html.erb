<p id="notice"><%= notice %></p>

<h1>Listing Profile Has Instruments</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Instrument</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_has_instruments.each do |profile_has_instrument| %>
      <tr>
        <td><%= profile_has_instrument.profile %></td>
        <td><%= profile_has_instrument.instrument %></td>
        <td><%= link_to 'Show', profile_has_instrument %></td>
        <td><%= link_to 'Edit', edit_profile_has_instrument_path(profile_has_instrument) %></td>
        <td><%= link_to 'Destroy', profile_has_instrument, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile has instrument', new_profile_has_instrument_path %>
